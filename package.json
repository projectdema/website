{
  "name": "projectdema-website",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 3000 --host",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:3000/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:3000/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@kurkle/color": "^0.1.9",
    "axios": "^0.26.1",
    "chart.js": "^3.7.1",
    "dotenv": "^16.0.0",
    "dsc.db": "^3.0.6",
    "lodash": "^4.17.21",
    "material-icons": "^1.10.8",
    "vue": "^3.2.31",
    "vue-navigation-bar": "^4.1.0",
    "vue-router": "^4.0.12",
    "vuesax": "^3.12.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.26",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^9.5.0",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.5.5",
    "vite": "^2.8.4",
    "vitest": "^0.5.0",
    "vue-tsc": "^0.31.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/projectdema/website.git"
  },
  "author": "projectdema",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/projectdema/website/issues"
  },
  "homepage": "https://github.com/projectdema/website#readme"
}
